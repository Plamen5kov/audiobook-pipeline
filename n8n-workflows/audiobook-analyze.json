    {
      "parameters": {
        "jsCode": "const result = $input.first().json;\nconst prev = $('extract text from webhook').first().json;\nconst job_id = prev.job_id;\nconst segments = result.segments || [];\nconst title = prev.title;\nconst started = $('write status (ar)').first().json.started_at;\nconst finished = Math.floor(Date.now() / 1000);\nreturn [{ json: { job_id, status_json: JSON.stringify({ phase: 'analyzing', status: 'done', job_id, segments, title, nodes: { \"text-analyzer\": { status: \"done\", started, finished } } }) } }];"
