        "jsCode": "const { job_id } = $input.first().json;\nconst started_at = Math.floor(Date.now() / 1000);\nreturn [{ json: { job_id, started_at, status_json: JSON.stringify({ phase: \"analyzing\", status: \"running\", job_id, nodes: { \"text-analyzer\": { status: \"running\", started: started_at } } }) } }];"
